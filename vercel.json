{
    "buildCommand": "npm run build",
    "outputDirectory": "out",
    "framework": null,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                },
                {
                    "key": "Cross-Origin-Opener-Policy",
                    "value": "same-origin"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.db)",
            "dest": "/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1.html"
        }
    ]
}
